{
  "name": "toolz-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test npm run eslint:fix && npx jest -i",
    "test:watch": "NODE_ENV=test npm run eslint:fix && npx jest -i --watchAll",
    "build": "NODE_ENV=production rm -rf ./dist && npx tsc -P ./tsconfig.json --outDir dist && babel dist -d dist",
    "start": "NODE_ENV=production NODE_TLS_REJECT_UNAUTHORIZED=0 node dist/server.js && npm run typeorm:dev migration:run",
    "dev": "NODE_ENV=dev ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
    "typeorm": "NODE_ENV=production ts-node ./node_modules/typeorm/cli",
    "typeorm:dev": "NODE_ENV=dev ts-node ./node_modules/typeorm/cli",
    "typeorm:test": "NODE_ENV=test ts-node ./node_modules/typeorm/cli",
    "migration:generate": "npm run build && npm run typeorm:dev migration:generate --",
    "migration:run": "npm run build && npm run typeorm:dev migration:run && npm run typeorm:test migration:run",
    "eslint": "npx eslint src",
    "eslint:fix": "npx eslint --fix src",
    "seed": "NODE_ENV=dev ts-node -r tsconfig-paths/register ./src/seeders",
    "seed:deploy": "NODE_ENV=production NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node -r tsconfig-paths/register ./src/seeders"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.16",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/winston": "^2.4.4",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-mock-extended": "^2.0.4",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@faker-js/faker": "^6.0.0-alpha.6",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express-async-errors": "^3.1.1",
    "express-handler-errors": "^2.3.1",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "morgan-body": "^2.6.6",
    "pg": "^8.7.3",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.41",
    "yup": "^0.32.11"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
